{
    "name": "endeavors/components-routing",
    "description": "Use Laravel Routing Component Independently",
    "type": "library",
    "license": "MIT",
    "require": {
        "illuminate/routing": "^4.2",
        "illuminate/events": "^4.2",
        "nesbot/carbon": "~1.0",
        "illuminate/html": "^4.2"
    },
    "require-dev": {
		"orchestra/testbench": "2.2.x",
        "phpunit/phpunit": "~4",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "psr-4": {
            "Endeavors\\Components\\Routing\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endeavors\\Components\\Routing\\Tests\\": "tests/"
        }
    }
}
